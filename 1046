#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;
int main()
{
     vector<int> v;
     int c;
     while(cin>>c)
     {
            v.push_back(c);
        for(int i=1;i<10;i++)
        {
            cin>>c;
            v.push_back(c);
        }
        sort(v.begin(),v.end());
        cout<<"max="<<v[9]<<endl;
        v.clear();
    }
}
 
