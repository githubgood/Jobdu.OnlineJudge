#include <iostream>
#include <cstdio>
#include <string>
using namespace std;
int main()
{
    int t,a,b,c,d;
    char f[10000];
    while(cin>>t)
    {
        while(t--)
        {
            cin>>f;
            sscanf(f,"%d.%d.%d.%d",&a,&b,&c,&d);
            if(a>=0&&a<=255&&b>=0&&b<=255&&c>=0&&c<=255&&d>=0&&d<=255)
            cout<<"Yes!"<<endl;
            else cout<<"No!"<<endl;
        }
    }
}
 
