#include <iostream>
using namespace std;
int main()
{
    int n,j,o,k;
    while(cin>>n)
    {
        j=0;
        o=0;
        while(n--)
        {
            cin>>k;
            if(k%2==0)o++;
            else j++;
        }
        if(o>j) cout<<"NO"<<endl;
        else cout<<"YES"<<endl;
    }   
}
