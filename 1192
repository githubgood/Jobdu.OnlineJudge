#include <iostream>
#include <string>
#include <algorithm>
using namespace std;
int main()
{
    string s,t;
    while(cin>>s)
    {
        t=s;
        string::iterator it;
        reverse(t.begin(),t.end());
        if(s==t)
        {
            cout<<"Yes!"<<endl;
        }
        else
        {
            cout<<"No!"<<endl;
        }
    }
}
 
