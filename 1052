#include <iostream>
using namespace std;
int main()
{
    int a[1000];
    int n,t,k,m,f;
    while(cin>>n)
    {
        f=1;
        for(int i=0;i<n;i++)
        {
            cin>>t;
            a[i]=t;
        }
        cin>>k;
        for(int i=0;i<n;i++)
        {
            if(k==a[i])
            {
              f=0;
                m=i;
                cout<<m<<endl;
            }
        }
        if(f==1)
        cout<<"-1"<<endl;
    }   
}
