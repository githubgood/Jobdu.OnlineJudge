#include <iostream> 
using namespace std;
int main()
{
    int y,m,d,t;
    while(cin>>y>>t)
    {
        if(y%400==0)
        {
            if(t<=31)
            {
                m=1;
                d=t;
            }
            else if(31<t&&t<=60)
            {
                m=2;
                d=t-31;         
            //  cout<<m<<" "<<d<<endl;
            }
                        else if(t>60&&t<=91)
            {
                m=3;
                d=t-31-29;              
            }
                        else if(t>91&&t<=121)
            {
                m=4;
                d=t-31-29-31;                   
            }
                        else if(t>121&&t<=152)
            {
                m=5;
                d=t-31-29-30-31;    
            }
                        else if(t>152&&t<=182)
            {
                m=6;
                d=t-29-31*3-30; 
            }   
            else if(t>182&&t<=213)
            {
                m=7;
                d=t-29-31*3-30*2;   
            }               
            else if(t>213&&t<=244)
            {
                m=8;
                d=t-29-31*4-30*2;   
            }   
            else if(t>244&&t<=274)
            {
                m=9;
                d=t-29-31*5-30*2;   
            }
                        else if(t>274&&t<=305)
            {
                m=10;
                d=t-29-31*5-30*3;   
            }
                else if(t>305&&t<=335)
            {
                m=11;
                d=t-29-31*6-30*3;   
            }
                    else if(t>335&&t<=366)
            {
                m=12;
                d=t-29-31*6-30*4;   
            }
        }
        else if((y%4==0)&&(y%100!=0))
        {
            if(t<=31)
            {
                m=1;
                d=t;
            }
            else if(31<t&&t<=60)
            {
                m=2;
                d=t-31;         
            }
                        else if(t>60&&t<=91)
            {
                m=3;
                d=t-31-29;              
            }
                        else if(t>91&&t<=121)
            {
                m=4;
                d=t-31-29-31;                   
            }
                        else if(t>121&&t<=152)
            {
                m=5;
                d=t-31-29-30-31;    
            }
                        else if(t>152&&t<=182)
            {
                m=6;
                d=t-29-31*3-30; 
            }   
            else if(t>182&&t<=213)
            {
                m=7;
                d=t-29-31*3-30*2;   
            }               
            else if(t>213&&t<=244)
            {
                m=8;
                d=t-29-31*4-30*2;   
            }   
            else if(t>244&&t<=274)
            {
                m=9;
                d=t-29-31*5-30*2;   
            }
                        else if(t>274&&t<=305)
            {
                m=10;
                d=t-29-31*5-30*3;   
            }
                else if(t>305&&t<=335)
            {
                m=11;
                d=t-29-31*6-30*3;   
            }
                    else if(t>335&&t<=366)
            {
                m=12;
                d=t-29-31*6-30*4;   
            }   
        }
        else
        {
            if(t<=31)
            {
                m=1;
                d=t;
            }
            else if(31<t&&t<=59)
            {
                m=2;
                d=t-31;         
            }
                        else if(t>59&&t<=90)
            {
                m=3;
                d=t-31-28;              
            }
                        else if(t>90&&t<=120)
            {
                m=4;
                d=t-31-28-31;                   
            }
                        else if(t>120&&t<=151)
            {
                m=5;
                d=t-31-28-30-31;    
            }
                        else if(t>151&&t<=181)
            {
                m=6;
                d=t-28-31*3-30; 
            }   
            else if(t>181&&t<=212)
            {
                m=7;
                d=t-28-31*3-30*2;   
            }               
            else if(t>212&&t<=243)
            {
                m=8;
                d=t-28-31*4-30*2;   
            }   
            else if(t>243&&t<=273)
            {
                m=9;
                d=t-28-31*5-30*2;   
            }
                        else if(t>273&&t<=304)
            {
                m=10;
                d=t-28-31*5-30*3;   
            }
                else if(t>304&&t<=334)
            {
                m=11;
                d=t-28-31*6-30*3;   
            }
                    else if(t>334&&t<=365)
            {
                m=12;
                d=t-28-31*6-30*4;   
            }
        }
        if(y<10)
        {
        cout<<"000";
        }
        if(10<=y&&y<100)
        {
        cout<<"00";
        }
        if(100<=y&&y<1000)
        {
        cout<<"0";
        }
        cout<<y<<"-";
        m>9?(cout<<m):(cout<<"0"<<m);
        cout<<"-";
        d>9?(cout<<d):(cout<<"0"<<d);
        cout<<endl;
    }
}
