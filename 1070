#include <iostream>
using namespace std;
int main()
{
    int y,m,d,t;
     
    while(cin>>y>>m>>d)
    {
        if(y%400==0)
        {
            if(m==1)
            {
                t=d;
            }
            else if(m==2)
            {
                t=31+d;            
            }
                        else if(m==3)
            {
                t=31+29+d;             
            }
                        else if(m==4)
            {
                t=31+29+31+d;              
            }
                        else if(m==5)
            {
            t=31+29+31+30+d;       
            }
                        else if(m==6)
            {
            t=31+29+31*2+30+d;     
            }          
            else if(m==7)
            {
            t=31+29+31*2+30*2+d;       
            }
            else if(m==8)
            {
            t=31+29+31*3+30*2+d;       
            }
            else if(m==9)
            {
            t=31+29+31*4+30*2+d;       
            }
                        else if(m==10)
            {
            t=31+29+31*4+30*3+d;       
            }
                        else if(m==11)
            {
            t=31+29+31*5+30*3+d;       
            }
                        else if(m==12)
            {
            t=31+29+31*5+30*4+d;       
            }
        }
        else if(y%4==0&&y%100!=0)
        {
                if(m==1)
            {
                t=d;
            }
            else if(m==2)
            {
                t=31+d;            
            }
                        else if(m==3)
            {
                t=31+29+d;             
            }
                        else if(m==4)
            {
                t=31+29+31+d;              
            }
                        else if(m==5)
            {
            t=31+29+31+30+d;       
            }
                        else if(m==6)
            {
            t=31+29+31*2+30+d;     
            }          
            else if(m==7)
            {
            t=31+29+31*2+30*2+d;       
            }
            else if(m==8)
            {
            t=31+29+31*3+30*2+d;       
            }
            else if(m==9)
            {
            t=31+29+31*4+30*2+d;       
            }
                        else if(m==10)
            {
            t=31+29+31*4+30*3+d;       
            }
                        else if(m==11)
            {
            t=31+29+31*5+30*3+d;       
            }
                        else if(m==12)
            {
            t=31+29+31*5+30*4+d;       
            }  
        }
        else
        {
                if(m==1)
            {
                t=d;
            }
            else if(m==2)
            {
                t=31+d;            
            }
                        else if(m==3)
            {
                t=31+28+d;             
            }
                        else if(m==4)
            {
                t=31+28+31+d;              
            }
                        else if(m==5)
            {
            t=31+28+31+30+d;       
            }
                        else if(m==6)
            {
            t=31+28+31*2+30+d;     
            }          
            else if(m==7)
            {
            t=31+28+31*2+30*2+d;       
            }
            else if(m==8)
            {
            t=31+28+31*3+30*2+d;       
            }
            else if(m==9)
            {
            t=31+28+31*4+30*2+d;       
            }
                        else if(m==10)
            {
            t=31+28+31*4+30*3+d;       
            }
                        else if(m==11)
            {
            t=31+28+31*5+30*3+d;       
            }
                        else if(m==12)
            {
            t=31+28+31*5+30*4+d;       
            }
        }
        cout<<t<<endl;
    }
}
