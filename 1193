#include <set>
#include <iostream>
using namespace std;
int main()
{
    long long a[100][100];
    int n;
    while(cin>>n)
    {
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
            {
                cin>>a[i][j];
            }
        }
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<n;j++)
            {
                if(j==0)
                cout<<a[j][i];
                else cout<<" "<<a[j][i];
            }
            cout<<endl;
        }
    }   
} 
